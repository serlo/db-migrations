{
  "name": "@serlo/server-migrate",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "author": "Serlo Education e.V.",
  "scripts": {
    "_prettier": "prettier . '!dist/**/*'",
    "build": "ts-node scripts/build",
    "delete-last-migration": "docker exec $(docker ps -qf 'name=mysql') mysql --password=secret --database=serlo --execute 'DELETE FROM migrations ORDER BY id DESC LIMIT 1'",
    "format:prettier": "yarn _prettier --write",
    "lint:prettier": "yarn _prettier --check",
    "list-migrations": "docker exec $(docker ps -qf 'name=mysql') mysql --password=secret --database=serlo --execute 'SELECT * FROM migrations'",
    "m": "yarn migrate:ts",
    "migrate": "docker-compose run --rm server-migrate",
    "migrate:ts": "DATABASE=mysql://root:secret@localhost:3306/serlo yarn db-migrate -m src/ up",
    "migrate:up": "yarn migrate up",
    "new": "./scripts/touch_file.sh",
    "push-image": "NODE_OPTIONS='--experimental-specifier-resolution=node' ts-node scripts/push-image",
    "test": "jest"
  },
  "dependencies": {
    "@serlo/markdown": "Entkenntnis/tmp-serlo-markdown",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "db-migrate-plugin-typescript": "^2.0.0",
    "htmlparser2": "^9.0.0",
    "ramda": "^0.29.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@edtr-io/plugin-text": "^3.0.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.19",
    "@types/ramda": "^0.29.3",
    "@types/react": "^18.2.15",
    "@types/semver": "^7.3.13",
    "@types/uuid": "^9.0.2",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "semver": "^7.5.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.7.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": "^18.0.0"
  }
}
